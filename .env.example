# MySQL
MYSQL_ROOT_PASSWORD=supersecretrootpass
MYSQL_DATABASE=guacamole_db
MYSQL_USER=guac_user
MYSQL_PASSWORD=guac_user_pass

# MySQL and guacd ports (override only if non-standard)
MYSQL_PORT=3306
GUACD_PORT=4822

# NGINX (update for your internal DNS hostname, e.g., guacamole.local; defaults to localhost)
SERVER_NAME=guacamole.local

# External ports (override if 80/443 are in use)
HTTP_PORT=80
HTTPS_PORT=443

# Logging (applies to all services; override to adjust)
LOG_MAX_SIZE=10m
LOG_MAX_FILE=3
